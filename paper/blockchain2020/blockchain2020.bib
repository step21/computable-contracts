@inproceedings{liScalablePrivacyPreservingDesign2019,
  title = {Scalable and {{Privacy}}-{{Preserving Design}} of {{On}}/{{Off}}-{{Chain Smart Contracts}}},
  booktitle = {2019 {{IEEE}} 35th {{International Conference}} on {{Data Engineering Workshops}} ({{ICDEW}})},
